<body id="top">


    <!-- preloader
    ================================================== -->
    <div id="preloader">
        <div id="loader" class="dots-fade">
            <div></div>
            <div></div>
            <div></div>
        </div>
    </div>
    <header class="s-header">
       <nav class="s-header__nav-wrap container mt-4">
            <div class="row justify-content-between">
                <div class="col-auto">
                    <a href="/" class="btn btn-primary">Home</a>
                </div>
                <div class="col-auto">
                    <a href="/admin/" class="btn btn-primary">Login</a>
                </div>
            </div>
        </nav>
    </header> <!-- Header End -->




    <div class="container">
        <form action="/admin/adminsignup" method="post">
            <h1 class="title text-center mt-4">Admin Registration</h1>
            <div class="mb-3">
                <label for="userId" class="form-label">User ID</label>
                <input type="text" class="form-control" name="user" id="userId" aria-describedby="userIdHelp">
                <div id="userIdHelp" class="form-text">Enter your user ID.</div>
            </div>

            <div class="mb-3">
                <label for="password" class="form-label">Password</label>
                <input type="password" name="pass" id="password" class="form-control">
            </div>
            <button type="submit" class="btn btn-primary mt-3">Submit</button>
        </form>
    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
